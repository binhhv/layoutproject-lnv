.auction h3{
    font-weight: normal;
}
.dataTables_filter, .dataTables_length, .dataTables_info{
	display: none;
}
.dataTables_paginate
{
    float: right;
}
#table-total-auction th{
    color: #007d9d;
}
.ps-current{
    min-height: 450px;
}
.ps-list ps-prev,ps-next{
    display: none;
}

.main-info-car
{
    padding-left: 5px !important;
}
.border{
    border: 1px solid #e8e7e7;
}
.border-left{
    border-left: 1px solid #e8e7e7;
}
.border-right{
    border-right: 1px solid #e8e7e7;
}
.border-top{
    border-top: 1px solid #e8e7e7;
}
.border-bottom{
    border-bottom: 1px solid #e8e7e7;
}
.non-border{
    border: none;
}

table#table-item-main-info-car{
        width: 100%;
}

table#table-item-main-info-car td.item{
    padding: 10px;
    border: 1px solid #e8e7e7;

}
table.sub-item td{
    padding: 5px;
}
table#table-item-main-info-car td
{
    font-size: 13px;
      color: #626262;
}
table#table-item-main-info-car td.row-item{
    width: 50%;
}
.item-color-auction{
    background-color: #f9fcfd;
}
.item-padding{
    padding-bottom: 10px;
}
input[type="button"].btn-auction{
    background: url(../images/background/background_btn_auction.png) no-repeat scroll 0 0 transparent;
    color: #007d9d;
    border: none;
    font-weight: normal;
   /* border: 1px solid #ccc;*/
   height: 31px;
    width: 100px;
    padding-left: 15px;
    padding-right:15px;
   
   
}
/*
.font-normal{
    color: #626262;
}
*/
.font-highlight{
    color: #626262;
    font-weight: bold;
}
.price{
    font-size: 25px;
    color: #cd4343;
}
.unit-money{
    color: #cd4343;
}
.title-car-auction
{
    border: 1px solid #e8e7e7;
    padding: 10px;
    color: #007d9d;
    background-color: #fafafa;
    margin-right: -5px !important;
}
.main-info{
    border-bottom: 1px solid #e8e7e7;

}
.sub-info{
    margin-top: 10px;
     border: 1px solid #e8e7e7;
}
.sub-info-2
{
    border-left: 1px solid #e8e7e7;
}
.sub-info-1{
     border: 1px solid #e8e7e7;
    padding: 15px;
    font-size: 13px;
}
.sub-info-2{
     border-top: 1px solid #e8e7e7;
     border-left: 1px solid #e8e7e7;
     border-right: 1px solid #e8e7e7;
    padding: 15px;
    font-size: 13px;
}
.row-item-sub-fino-1{
      border: 1px solid #e8e7e7;
    padding: 10px;
}
.row-features{
    padding: 10px;
    background-color: #fafafa;
}
.nopadding-subinfo{
    margin: 0 !important;
    padding: 0 !important;
}
.row-specifications-left{
/*     border-right: 1px solid #e8e7e7;*/
}
.row-specifications{
    border-top: 1px solid #e8e7e7;
    padding-top: 10px;
}
.item-text-auction-reponsive{
    word-wrap: break-word;
}
.text-left{
    text-align:left;
     word-wrap: break-word;
}
.row-detail-specification{
    display: table;
}
.row-specifications{
  
     float: none;
    display: table-cell;
    vertical-align: top;
}
.row-specifications-border-sprilit{
      border-right: 1px solid #e8e7e7;
}
/*DETAIL CAR*/
table.table-contact td{
    padding: 5px;
}
.row-advance{
        border: 1px solid #e8e7e7;
    margin-top: 10px;
}
.seller{
    border: 1px solid #e8e7e7; 
}
.support{
   padding: 5px !important;
}
.title-seller{
       border-bottom: 1px solid #e8e7e7;   
    background-color: #fafafa;
    padding: 10px;
}
table.table-seller td{
    color: #626262;
     font-size: 13px;
    padding: 5px;
}
table.table-seller td.avatar{
    width: 30%;
}
table.table-seller td.seller-intro{
    word-wrap: break-word;
}
table.table-seller td.avatar div{
    
display: table-cell;

text-align: center;
vertical-align: middle;
    width: 60px;
    height: 60px;
    background:  url(../images/background/background_avatar_seller.png);
}
table.table-seller td.avatar div img{


    width: 40px;
    height: 40px;
        max-height: 100%;
  max-width: 100%;
}
.detail-support-service{
    padding: 10px !important;
}
.detail-support-service div{
   text-align: center;
}
@media screen and (min-width: 768px) {
  .row-advance, .row-advance > div[class*='col-'] {  
      display: -webkit-box;
      display: -moz-box;
      display: -ms-flexbox;
      display: -webkit-flex;
      display: flex;
      flex:1 1 auto;
  }

}


/*REPONSIVE SETUP*/
@media screen and (max-width: 767px) {
    
    .item-text-auction-reponsive{
        text-align: left;
    }
    .main-info{
    border-bottom: none;

}
    .sub-info-2
{
    border-left: none;
}
