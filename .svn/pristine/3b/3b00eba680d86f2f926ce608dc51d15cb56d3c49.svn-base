.map-company{
    min-height: 350px;
}
.contact-us-content{
/*    min-height: 500px;*/
    color: #626262;
}
.title-page-content h2{
    font-weight: normal;
}

.contact-us-content .company-name-content{
    font-size: 15px;
    font-weight: bold;
}
.address-content{
    background: white url(../images/icon/icon_address_company.png) no-repeat 1% center;
    padding-left: 45px;
}
.tel-content{
    background: white url(../images/icon/icon_phone_company.png) no-repeat 1% center;
    padding-left: 45px;
}
    
.email-content{
    background: white url(../images/icon/icon_mail_company.png) no-repeat 1% center;
    padding-left: 45px;
}

.form-group-contact > label{
    text-align: left;
    width: 100%;
    margin-top: 10px;
    font-weight: normal;
}
.form-group-contact input[type="text"],select{
    
    text-align: left;
    width: 100%;
    
}
.form-group-contact input[type="text"],select{
  
  border-radius: 20px;
  font-size: 12px;
  padding: 5px;
  
}
.form-group-contact textarea{
     border-radius: 10px;
    font-size: 12px;
    padding: 5px;
    width: 90%;
}
.form-group-contact input[type="submit"]{
  border: 2px solid #FFFFFF;
  border-radius: 20px;
  font-size: 12px;
  padding:  5px;
  background-color: #f3f6f9;
  border: 1px solid white;
  text-align: left;
  width: 80px;
  text-align: center;
  color: #007d9d;
  font-weight: bold;
  line-height: 20px;  
  height: 30px;

}
.form-group-contact input[type="button"]{
      border: 2px solid #FFFFFF;
      border-radius: 20px;
      font-size: 12px;
      padding: 5px 5px 5px 50px;
      background-color: #f3f6f9;
      border: 1px solid white;
      text-align: left;
      width: 100%;
      text-align: center;
      color: #007d9d;
      font-weight: bold;
      text-align: left;  
      line-height: 20px;  
      height: 30px;
}
.form-group-contact select{
   
		-webkit-appearance: none;  /*Removes default chrome and safari style*/
		-moz-appearance: none; /* Removes Default Firefox style*/
		background: #0088ab url(../images/select-arrow.png) no-repeat 98% center;
}
.form-group-contact select.small-select{
		background: #0088ab url(../images/select-arrow.png) no-repeat 95% center;
}
.form-group-contact textarea { height: 150px;resize: none;}
.form-group-contact p{
    width: 2%;
    float: left;
}
.row-contact-us .row{
    overflow: hidden; 
}

.row-contact-us [class*="col-"]{
    margin-bottom: -99999px;
    padding-bottom: 99999px;
}
.send-contact{
    margin-top: 10px !important;
    margin-left: 15px !important;
}