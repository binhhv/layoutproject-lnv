/*MENU HEADER*/
#header-menu{
    margin-top: 10px;
   
}
#logo {
    display:inline-block;
    width: 200px;
    height: auto;
   
    position:relative;
}
#menu {
    position: absolute;
    right: 0;
    top: 15px;
    display:none;
    cursor: pointer;
}
#nav {
    list-style: none;
    padding: 0;
    display: inline-block;
}
#nav.vertical {
    display: block;
    padding-left: 20px;
}
#nav.vertical li {
    display: list-item;
    line-height: 40px;
}
#nav li {
    display:inline-block;
    padding-right: 40px;
}
#nav li a{
    text-decoration: none;
    color: black;
    font-weight: bold;
}

#nav li:last-child {
    padding-right: 0;
}
@media screen and (max-width: 768px) {
    #menu {
        display: inline-block;
    }
    #nav {
        display: none;
    }
    #logo{
        width: 100%;
    }
    
    
}


#nav li a.active{
    color: #256730;
    border-bottom: 3px solid #256730;
}
@media (max-width:960px) and (min-width:768px) {
    #logo {
       width: 100%;
        text-align: center;
    }
}


/*sub menu*/
#nav ul {
/*    visibility: hidden;*/
    position: absolute;
    padding-left: 0px;
    background-color: #e7eef2;
}

/*
#nav li:hover > ul {
    opacity: 1;
    visibility: visible;
    margin: 0;
}
*/



#nav ul li {
    float: none;
    display: block;
    padding-right: 0px;
    border: 0;
    _line-height: 0; /*IE6 only*/
}



#nav ul a {    
    padding: 10px;
    width: 130px;
    _height: 10px; /*IE6 only*/
    display: block;
    white-space: nowrap;
    float: none;
    text-transform: none;
}

#nav ul a:hover {
   color: #007d9d;
   
}




#menu-trigger { /* Hide it initially */
    display: none;
}

@media screen and (max-width: 767px) {
    #nav ul a
    {
        padding: 0px;
    }
     #nav ul {
        position: static;
        visibility: visible;
        opacity: 1;
        margin: 0;
        background: none;
        box-shadow: none;               
    }
    #nav ul li{
         list-style-type: none;
        margin-left: 20px;
        box-shadow: none;       
    }
          

}


