/*SHARE BUTTON MEDIA*/
.share-btn-wrp {
	list-style: none;
	display: block;
	margin: 0px;
	padding: 0px;
	width: 32px;
    top: 150px;
	right: 0px;
	position: fixed;
    z-index: 9999;
}
.share-btn-wrp .button-wrap{
	text-indent:-100000px;
	width:32px;
	height: 32px;
	cursor:pointer;
	transition: width 0.1s ease-in-out;
}
.share-btn-wrp > .facebook{
	background: url(../images/share-button/share-icons.png) no-repeat -42px 0px;
}
.share-btn-wrp > .facebook:hover{
	background: url(../images/share-button/share-icons.png) no-repeat -4px -0px;
	width:38px;
}
.share-btn-wrp > .twitter{
	background: url(../images/share-button/share-icons.png) no-repeat -42px -34px;
}
.share-btn-wrp > .twitter:hover{
	background: url(../images/share-button/share-icons.png) no-repeat -4px -34px;
	width:38px;
}
@media screen and (min-width: 768px) {
  .content-index, .content-index > div[class*='col-'] {  
      display: -webkit-box;
      display: -moz-box;
      display: -ms-flexbox;
      display: -webkit-flex;
      display: flex;
      flex:1 1 auto;
  }


}
@media screen and (max-width: 767px) {
    .share-btn-wrp {
	list-style: none;
	display: block;
	margin: 0px;
	padding: 0px;
	width: 32px;
    top: 300px;
	right: 0px;
	position: fixed;
    z-index: 9999;
}
 
