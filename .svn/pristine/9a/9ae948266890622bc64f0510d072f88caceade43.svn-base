.content{
    min-height: 500px;
}
.form-group-faq > label{
    text-align: left;
    width: 100%;
    margin-top: 10px;
}
.form-group-faq input[type="text"],select{
    text-align: left;
    width: 100%;
    
}
.form-group-faq input[type="text"],select{
  border-radius: 20px;
  font-size: 12px;
  padding: 5px;
}
.form-group-faq input[type="submit"]{
  border-radius: 20px;
  font-size: 12px;
  padding:  5px;
  text-align: left;
  width: 80px;
  text-align: center;
  font-weight: bold;
  line-height: 20px;  
  height: 30px;

}
.form-group-faq input[type="button"]{
      border: 2px solid #FFFFFF;
      border-radius: 20px;
      font-size: 12px;
      padding: 5px 5px 5px 50px;
      background-color: #f3f6f9;
      border: 1px solid white;
      text-align: left;
      width: 100%;
      text-align: center;
      color: #007d9d;
      font-weight: bold;
      text-align: left;  
      line-height: 20px;  
      height: 30px;
}
.form-group-faq select{
   
		-webkit-appearance: none;  /*Removes default chrome and safari style*/
		-moz-appearance: none; /* Removes Default Firefox style*/
		background: #0088ab url(../images/select-arrow.png) no-repeat 98% center;
}
.form-group-faq select.small-select{
		background: #0088ab url(../images/select-arrow.png) no-repeat 95% center;
}
.form-group-faq p{
    width: 2%;
    float: left;
}
.faq-left{
    text-align: left;
}
.send-faq{
    margin-top: 10px !important;
}
.panel-heading{
        height: 50px;

    font-size: 20px;
    font-weight: bold;
    background: #fafafa url(../images/icon/icon_faq.png) no-repeat 1% center;
    padding: 0 0 0 50px;
    margin: 0;
   
}
.panel-heading .panel-title{
        color:#007d9d;
    line-height: 50px;
}
