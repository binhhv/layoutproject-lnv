*/
}
@media screen and (min-width: 768px) {
     #login-form{
        display: none;
    }
    #login-dialog{
        display: block;
    }
    .item-text-auction-reponsive{
        text-align: left;
    }
      
}
@media screen and (min-width: 991px) {
     #login-form{
        display: block;
    } 
    #login-dialog{
        display: none;
    }
}