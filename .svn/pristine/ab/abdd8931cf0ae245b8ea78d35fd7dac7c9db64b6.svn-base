@media only screen and (max-width: 960px) and (min-width: 768px)
{
    .page-link{
    text-align: left;
    position: inherit;
    bottom: 0;
    padding-right: 0;
    }
      
  #crumb{
    float: left;
     margin-left: -30px;
}
}
@media only screen and (max-width: 520px) and (min-width: 480px)
{
     .item-car{
  height: 90px;
    }
 .item-car img{
        height: 80px;
            display: inline-block;
    }
   
}


@media only screen and (max-width: 479px) and (min-width: 320px)
{
     .item-car{
  height: 100px;
    }
 .item-car img{
        height: 90px;
            display: inline-block;
    }
    .detail-car-button{
        display: none;
    }
    .item-car .name-product-left{
        font-size: 9px;
    }
