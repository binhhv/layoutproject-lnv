}

@media screen and (max-width:320px){
         .item-car{
  height: 60px;
    }
 .item-car img{
        height: 50px;
            display: inline-block;
    }
    .detail-car-button{
        display: none;
    }
    .item-car .name-product-left{
        font-size: 6px;
    }
