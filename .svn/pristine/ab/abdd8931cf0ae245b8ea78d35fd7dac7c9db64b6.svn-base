@media screen and (max-width: 767px) {
    #login-form{
        display: none;
    }
     #login-dialog{
        display: block;
    }
    #header-hotline p{
        text-align: left;
    }
    #header-change-language p{
        text-align: left;
    }
     #header-change-language
    {
        margin-bottom: 10px;
    }
    #header-login{
        text-align: left;
    }
    header.container{
    width: 100%;
    padding: 0;
    }
    .image-car{
        text-align: left;
         width: auto;
        position:  relative;
    }
    .image-car img{
        position: absolute;
    }
   .content-background{
         background:white ;
       margin-top: 5px;
       height: auto;
       
   }
  .reponsive-background{
      display: block;
  }
    .car{
        margin: 0 auto;
        overflow: hidden;
    }
 

    .item-car{
  display: block;
/*  width: 100%;*/
  height: 120px;
  line-height: 190px;
        text-align: center;
    }
    .item-car img{
              max-width: 100%;
        width: 100%;
/*        height: 120px;*/
            display: inline-block;
        margin: 3px !important;
    }
    .nopadding{
        padding: 3px !important;
    }
    body, html{
        overflow-x: hidden
    }
.name-product-left{
    margin-left: 10px;
    margin-top: 5px;
}
    .footer-ads{
        text-align: lef;
        margin-top: 10px;
    }
    .footer-news{
        margin-top: 10px;
    }
    .footer-links{
        margin-top: 10px;
    }
    .footer-links ul {
        padding: 0;
    }
    .page-link{
    text-align: left;
    position: inherit;
    bottom: 0;
    padding-right: 0;
    }
    
  #crumb{
    float: left;
     margin-left: -45px;
}
    .item-text-auction-reponsive{
        text-align: left;
    }
}
