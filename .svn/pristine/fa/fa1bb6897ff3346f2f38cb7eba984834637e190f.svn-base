.row-item-car{
     border: 1px solid #e8e7e7;
   
}
.new-row-item{
     margin-top: 5px !important;
}
.name-car{
     border-bottom: 1px solid #e8e7e7;
    padding: 10px;
    color: #007d9d;
    background-color: #fafafa;
}

.left-item-car{
    border-right: 1px solid #e8e7e7;
     padding: 0 !important;
}
.row-item{
    width: 100%;
}
.item-image-car{
            display: table-cell;
        vertical-align: middle;
        text-align: center;
}
.row-item-advance{
     border-bottom: 1px solid #e8e7e7;
}
.row-item-advance div{
     width: 100%;
    padding: 10px !important;
}
.item-image-car img{
    height: 90px;
    width: 250px;
    max-width: 100%;
    max-height: 100%;
        display: block;
   margin: auto auto;
}
.item-intro-car{
    padding: 10px;
}
.font-highlight{
    color: #626262;
    font-weight: bold;
}
.price{
    font-size: 25px;
    color: #cd4343;
}
.unit-money{
    color: #cd4343;
}
input[type="button"].btn-detail{
    background: url(../images/background/background_btn_auction.png) no-repeat scroll 0 0 transparent;;
    color: #007d9d;
    border: none;
    font-weight: normal;
   /* border: 1px solid #ccc;*/
   height: 31px;
    width: 100px;
    padding-left: 15px;
    padding-right:15px;
    float: right;
   
   
}
 .nopadding-item,.right-item-car{
     padding: 0 !important;
     margin: 0 !important;
 }
table{
    width: 100%;
}
table td{
    color: #626262;
     font-size: 13px;
    padding: 5px;
}
table td.right-text-td{
    text-align: right;
     font-weight: bold;
}
.main-info{
 border-top: 1px solid #e8e7e7;
    margin: 0 auto;
}
.left-main-info,.right-main-info{
    padding: 0 !important;
    border-bottom: 1px solid #e8e7e7;
    background-color: #fafafa;
}
.left-main-info{
     border-right: 1px solid #e8e7e7;
}
@media screen and (min-width: 768px) {
  .row-item-car, .row-item-car > div[class*='col-'] {  
      display: -webkit-box;
      display: -moz-box;
      display: -ms-flexbox;
      display: -webkit-flex;
      display: flex;
      flex:1 1 auto;
  }
    .row-item-image-detail, .row-item-image-detail > div.item-image-car,div.item-intro-car{
         display: -webkit-box;
      display: -moz-box;
      display: -ms-flexbox;
      display: -webkit-flex;
      display: flex;
      flex:1 1 auto;
    }
/*
     .row-item-image-detail, .row-item-image-detail > div[class*='col-'] {  
      display: -webkit-box;
      display: -moz-box;
      display: -ms-flexbox;
      display: -webkit-flex;
      display: flex;
      flex:1 1 auto;
  }
*/
}
/*REPONSIVE SETUP*/
@media screen and (max-width: 767px) {
    
  .left-item-car{
       border: 1px solid #e8e7e7;
     
  }
    .right-item-car{
         border: 1px solid #e8e7e7;
    }
    .item-image-car img{
    height: 150px;
    width: 250px;
    max-width: 100%;
    max-height: 100%;

}
    .left-main-info,.right-main-info{
        border: 1px solid #e8e7e7;
    }
}

